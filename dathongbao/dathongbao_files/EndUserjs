function DeleteAction(url, mesage) {
    if (mesage == null || mesage == '') {
        mesage = "Bn xc nhn thc hin thao tc ny ?";
    }
    $.confirm({
        title: 'Xc nhn xa',
        content: mesage,
        draggable: false,
        theme: 'material',
        buttons: {
            confirm: {
                btnClass: 'btn-primary',
                text: "Xc nhn",
                action: function () {
                    AjaxCall(url, 'post', null, function (rs) {
                        if (rs.Status) {
                            NotiSuccess("Thnh cng", rs.Message);
                            AfterSussessActionAjaxform();
                        } else {
                            NotiError("Li x l", rs.Message);
                        }
                    })
                }
            },
            cancel: {
                text: "ng",
                action: function () {

                }
            }
        }
    });
    //bootbox.confirm(mesage, function (result) {
    //    if (result) {
    //        AjaxCall(url, 'post', null, function (rs) {
    //            if (rs.Status) {
    //                NotiSuccess("Thnh cng", rs.Message);
    //                AfterSussessActionAjaxform();
    //            } else {
    //                NotiError("Li x l", rs.Message);
    //            }
    //        })
    //    }
    //});
}
function RegularAction(url, mesage) {
    if (mesage == null || mesage == '') {
        mesage = "Bn xc nhn thc hin thao tc ny ?";
    }
    $.confirm({
        title: 'Xc nhn thao tc',
        content: mesage,
        draggable: false,
        theme: 'material',
        buttons: {
            confirm: {
                btnClass: 'btn-primary',
                text: "Xc nhn",
                action: function () {
                    AjaxCall(url, 'post', null, function (rs) {
                        if (rs.Status) {
                            NotiSuccess("Thnh cng", rs.Message != null ? rs.Message:"Thao tc thc hin thnh cng");
                            AfterSussessActionAjaxform();
                        } else {
                            NotiError("Li x l", rs.Message);
                        }
                    })
                }
            },
            cancel: {
                text: "ng",
                action: function () {

                }
            }
        }
    });
}

